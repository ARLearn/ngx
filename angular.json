{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-arlearn": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "../gae/src/main/webapp",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "src/main.scss"
            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.min.js",
              "./node_modules/gsap/dist/CSSRulePlugin.js",
              "./node_modules/gsap/dist/Draggable.js",
              "./node_modules/gsap/dist/EaselPlugin.js",
              "./node_modules/gsap/dist/EasePack.js",
              "./node_modules/gsap/dist/MotionPathPlugin.js",
              "./node_modules/gsap/dist/PixiPlugin.js",
              "./node_modules/gsap/dist/ScrollToPlugin.js",
              "./node_modules/gsap/dist/TextPlugin.js",
              "./node_modules/gsap/dist/gsap.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "youplay": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.youplay.ts"
                }
              ],
              "index": "src/index.html",
              "styles": [
                {
                  "input": "src/main.youplay.scss"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "youplay-prod": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.youplay.ts"
                }
              ],
              "index": "src/index.html",
              "styles": [
                {
                  "input": "src/main.youplay.scss"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/webinf/youplay/WEB-INF/",
                  "output": "/WEB-INF/"
                },
                {
                  "glob": "**/*",
                  "input": "src/assets",
                  "output": "/assets"
                }
              ]
            },
            "nbd": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.nbd-local.ts"
                }
              ],
              "index": "src/index.nbd.html",
              "styles": [
                {
                  "input": "src/main.nbd.scss"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "nbd-prod": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.nbd.ts"
                }
              ],
              "index": "src/index.html",
              "styles": [
                "src/main.nbd.scss"
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/webinf/nbd/WEB-INF/",
                  "output": "/WEB-INF/"
                },
                {
                  "glob": "**/*",
                  "input": "src/assets",
                  "output": "/assets"
                }
              ]
            },
            "nbd-dev-remote": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.nbd-dev-remote.ts"
                }
              ],
              "index": "src/index.html",
              "styles": [
                "src/main.nbd.scss"
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false,
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/webinf/nbd-dev/WEB-INF/",
                  "output": "/WEB-INF/"
                },
                {
                  "glob": "**/*",
                  "input": "src/assets",
                  "output": "/assets"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-arlearn:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-arlearn:build:production"
            },
            "youplay": {
              "browserTarget": "ng-arlearn:build:youplay"
            },
            "nbd": {
              "browserTarget": "ng-arlearn:build:nbd"
            },
            "nbd-dev": {
              "browserTarget": "ng-arlearn:build:nbd-dev-remote"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-arlearn:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              },
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-arlearn-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng-arlearn:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-arlearn:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-arlearn",
  "cli": {
    "defaultCollection": "@ngrx/schematics",
    "analytics": "e3da1f34-60d5-4ad3-827d-696e427f37a4"
  }
}
