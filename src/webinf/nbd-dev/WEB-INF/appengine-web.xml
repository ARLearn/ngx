<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>serious-gaming-platform-dev</application>

    <runtime>java8</runtime>
    <version>1</version>
    <threadsafe>true</threadsafe>
    <inbound-services>
        <service>mail</service>
    </inbound-services>
    <sessions-enabled>true</sessions-enabled>
    <staging>
        <enable-jar-classes>false</enable-jar-classes>
    </staging>
    <env-variables>
      <env-var name="ENDPOINTS_SERVICE_NAME" value="serious-gaming-platform-dev.appspot.com" />
    </env-variables>
</appengine-web-app>
